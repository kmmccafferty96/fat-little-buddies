    "commitlint": "^12.1.4",
    "husky": "^4.2.5",
    "postcss": "^8.3.4",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "tailwindcss": "^2.1.4",
    "typescript": "~4.2.4"
